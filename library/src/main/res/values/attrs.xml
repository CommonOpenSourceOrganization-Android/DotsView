<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DotsView">
        <attr name="dv_colorSelected" format="color" />
        <attr name="dv_colorUnselected" format="color" />
        <attr name="dv_dotSpace" format="dimension" />
        <attr name="dv_dotRadius" format="dimension" />
        <attr name="dv_count" format="integer" />
        <attr name="dv_current" format="integer" />
        <attr name="dv_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
</resources>